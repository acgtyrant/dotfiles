# Simplify Your Life With an SSH Config File
# http://nerderati.com/2011/03/17/simplify-your-life-with-an-ssh-config-file/

Host github.com
  User git
  Hostname github.com
  Port 22
  IdentityFile ~/.ssh/id_rsa

Host do
  User acgtyrant
  Hostname 104.236.158.105
  Port 22
  IdentityFile ~/.ssh/id_rsa

Host alc
  User acgtyrant
  Hostname build.archlinuxcn.org
  Port 22
  IdentityFile ~/.ssh/id_rsa

Host yy
  User geysh
  Hostname 10.10.24.76
  Port 22
  IdentityFile ~/.ssh/id_rsa
