# Simplify Your Life With an SSH Config File
# http://nerderati.com/2011/03/17/simplify-your-life-with-an-ssh-config-file/

Host github.com
  User git
  Hostname github.com
  Port 22
  IdentityFile ~/.ssh/id_rsa

Host yy
  User acgtyrant
  Hostname 192.168.144.101
  Port 22
  IdentityFile ~/.ssh/id_rsa
  ForwardAgent yes

Host aa
  User acgtyrant
  Hostname 192.168.144.70
  Port 22
  IdentityFile ~/.ssh/id_rsa
  ForwardAgent yes

Host dd
  User acgtyrant
  Hostname 192.168.144.50
  Port 22
  IdentityFile ~/.ssh/id_rsa
  ForwardAgent yes

Host jiyou
  User acgtyrant
  Hostname 45.32.66.91
  Port 22

Host data
  User acgtyrant
  Hostname 10.10.40.112
  Port 22
