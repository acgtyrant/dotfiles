# Simplify Your Life With an SSH Config File
# http://nerderati.com/2011/03/17/simplify-your-life-with-an-ssh-config-file/

Host github.com
  User git
  Hostname github.com
  Port 22
  IdentityFile ~/.ssh/id_rsa

Host xx
  User acgtyrant
  Hostname 10.10.24.92
  Port 22
  IdentityFile ~/.ssh/id_rsa

Host yy
  User acgtyrant
  Hostname 172.22.52.68
  Port 22
  IdentityFile ~/.ssh/id_rsa
  ForwardAgent yes

